    }
  },
  "autoload-dev": {
    "psr-0": {
      "Eix": "src/php/test"
    }
  },
  "prefer-stable": true,
  "config": {
    "vendor-dir": "src/php/lib"
  },
  "require-dev": {
    "phing/phing": "*",
    "phpunit/phpunit": "~5",
    "phplint/phplint": "*",
    "phploc/phploc": "*",
    "sebastian/phpcpd": "*",
    "mayflower/php-codebrowser": "*"
  }
