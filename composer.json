{
  "name": "eix/contact",
  "description": "Eixtension that provides contact form interaction.",
  "type": "library",
  "keywords": [
    "contact form",
    "e-mail",
    "contact",
    "customer",
    "communication"
  ],
  "homepage": "https://github.com/eix/contact",
  "time": "2013-01-01",
  "license": "MIT",
  "authors": [
    {
      "name": "Max No√©",
      "email": "code@0h.io",
      "role": "Developer"
    }
  ],
  "support": {
    "source": "https://github.com/eix/contact",
    "issues": "https://github.com/eix/contact/issues"
  },
  "require": {
    "eix/core": "dev-master"
  },
  "autoload": {
    "psr-0": {
